pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Compiling the code...'
            }
        }

        stage('Unit Test') {
            steps {
                echo 'Running unit tests...'
            }
        }

        stage('Staging Deploy') {
            steps {
                echo 'Deploying to staging environment...'
            }
        }
    }
}
